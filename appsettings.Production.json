{
  "AppName": "Vehicle Service",
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft.AspNetCore": "Error"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=prod-db-server;Username=prod_user;Password=prod_password;Database=prod_db"
  },
  "CarsConfig": {
    "AllowCreate": true,
    "EnableGetAll": true,
    "MaxCarsLimit": 10
  },
  "OrdersConfig": {
    "EnableGetAll": true,
    "MaxOrdersLimit": 10
  }
}